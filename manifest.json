{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "SpeakEasy",
        "version": "0.0.5",
        "minimum-overwolf-version": "0.77.1.0",
        "author": "Adam Keenan",
        "icon": "IconMouseOver.png",
        "icon_gray": "IconMouseNormal.png",
        "description": "Test App"
    },
    "data": {
        "start_window": "main",
        "windows": {
            "main": {
                "file": "Files/index.html",
                "transparent": true,
                "resizable": false,
                "size": { "width": 188, "height": 400 },
            },
            "channels": {
                "file": "Files/channels.html",
                "transparent": true,
                "show_in_taskbar": true,
                "resizable": false,
                "size": { "width": 188, "height": 400 },
            },
            "toggles": {
                "file": "Files/toggles.html",
                "transparent": true,
                "show_in_taskbar": true,
                "resizable": false,
                "size": { "width": 188, "height": 400 },
            },
            "chat": {
                "file": "Files/chat.html",
                "transparent": true,
                "show_in_taskbar": true,
                "resizable": false,
                "size": { "width": 427, "height": 206 },
            },
            "clients": {
                "file": "Files/clients.html",
                "transparent": true,
                "show_in_taskbar": true,
                "resizable": false,
                "size": { "width": 400, "height": 400 },
            },
            "notifications": {
                "file": "Files/notifications.html",
                "transparent": true,
                "show_in_taskbar": false,
                "resizable": false,
                "size": { "width": 188, "height": 200 },
            },
            "help": {
                "file": "Files/help.html",
                "transparent": true,
                "show_in_taskbar": false,
                "resizable": false,
                "size": { "width": 600, "height": 400 },
            },
        }
    }
}
