{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "SpeakEasy",
        "version": "0.0.5",
        "minimum-overwolf-version": "0.77.1.0",
        "author": "Adam Keenan",
        "icon": "IconMouseOver.png",
        "icon_gray": "IconMouseNormal.png",
        "description": "Test App"
    },
    "data": {
        "start_window": "main",
        "windows": {
            "main": {
                "file": "Files/index.html",
                "transparent": true,
                "resizable": true,
                "size": { "width": 200, "height": 400 },
                "min_size": { "width": 0, "height": 0 },
            },
            "channels": {
                "file": "Files/channels.html",
                "transparent": true,
                "resizable": false,
                "size": { "width": 200, "height": 400 },
            },
            "settings": {
                "file": "Files/settings.html",
                "transparent": true,
                "resizable": false,
                "size": { "width": 200, "height": 400 },
            },
        }
    }
}
